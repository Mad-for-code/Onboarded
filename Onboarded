
public void insert(String rowKey, Boolean value) {
    try {
        // Create a mutation to apply to the row
        RowMutation mutation = RowMutation.create("your-table-id", rowKey);

        // Check if the value is not null
        if (value != null) {
            // Insert the value in the 'cf1' column family and 'status' column
            mutation.setCell("cf1", "status", value.toString());
        }

        // Apply the mutation to the table
        dataClient.mutateRow(mutation);

        System.out.println("Row inserted successfully.");
    } catch (Exception e) {
        System.err.println("Failed to insert row: " + e.getMessage());
        e.printStackTrace();
    }
}
